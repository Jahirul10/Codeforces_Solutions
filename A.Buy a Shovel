    #include<bits/stdc++.h>
    #define ll long long int
    using namespace std;
    int main()
    {
        int k,r,i=1;
        cin>>k>>r;
        int a=k;
        while(i)
        {
            if(k%10==r || k%10==0)
            {
                cout<<i<<endl;
                break;
            }
            i++;
            k=a*i;
        }
        return 0;
    }
