    #include<bits/stdc++.h>
    using namespace std;
    void solve()
    {
        int t;
        cin>>t;
        while(t--)
        {
            int a,b,c,d,cnt=0;
            cin>>a>>b>>c>>d;
            if(a<b)cnt++;
            if(a<c)cnt++;
            if(a<d)cnt++;
            cout<<cnt<<endl;
     
        }
    }
    int main()
    {
        solve();
        return 0;
    }
